 } = await axios.get(`/api/products/${match.params.id}`)\n    //   setProduct(data)\n    // }\n    // fetchProduct();\n\n    dispatch(listProductDetails(match.params.id));\n  }, [dispatch, match])\n\n  const addToCartHandler = () => {\n    history.push(`/cart/${match.params.id}?qty=${qty}`)\n  }\n  return (\n    <>\n      <Link className='btn btn-light my-3' to='/'>\n        Go Back\n      </Link>\n      { loading ? (\n        <Loader />\n      ) : error ? (\n        <Message variant='danger'>{error}</Message> \n      ) : (\n        <Row>\n        <Col md={6}>\n          <Image src={product.image} alt={product.name} fluid />\n        </Col>\n        <Col md={3}>\n          <ListGroup variant='flush'>\n            <ListGroup.Item>\n              <h3>{product.name}</h3>\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <Rating value={product.rating} text={`${product.numReviews} reviews`}/>\n            </ListGroup.Item>\n            <ListGroup.Item>\n              Price: ${product.price}\n            </ListGroup.Item>\n            <ListGroup.Item>\n              Description: ${product.description}\n            </ListGroup.Item>\n          </ListGroup>\n        </Col>\n        <Col md={3}>\n          <Card>\n            <ListGroup variant='flush'>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Price:</Col>\n                  <Col><strong>${product.price}</strong></Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Status:</Col>\n                  <Col>${product.countInStock > 0 ? 'In Stock' : 'Out of Stock'}</Col>\n                </Row>\n              </ListGroup.Item>\n            </ListGroup>\n            {product.countInStock > 0 && (\n              <ListGroup.Item>\n                <Row>\n                  <Col>Qty</Col>\n                  <Col>\n                    <Form.Control as='select' value={qty} onChange={(e) => {\n                      setQty(e.target.value)}}>\n                      {[...Array(product.countInStock).keys()].map(x => {\n                        return <option key={x + 1} value={x + 1}>\n                          {x + 1}\n                        </option>\n                      })}\n                    </Form.Control>\n                  </Col>\n                </Row>\n              </ListGroup.Item>\n            )}\n            <ListGroup.Item>\n              <Button onClick={addToCartHandler} className='btn-block' type='button' disabled={product.countInStock === 0}>\n                Add to cart\n              </Button>\n            </ListGroup.Item>\n          </Card>\n        </Col>\n      </Row>\n      )\n      }\n    </>\n  )\n}\n\nexport default ProductScreen\n","/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/store.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/constants/productConstants.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/actions/productActions.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/Loader.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/Message.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/CartScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/reducers/productReducers.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/reducers/cartReducers.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/constants/cartConstants.js",[],["91","92"],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/actions/cartActions.js",[],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"93","replacedBy":"97"},{"ruleId":"95","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":8,"column":30,"nodeType":"101","messageId":"102","endLine":8,"endColumn":42},{"ruleId":"93"