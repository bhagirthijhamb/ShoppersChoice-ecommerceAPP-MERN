ntend/src/screens/HomeScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/products.js",[],["186","187"],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/Product.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/Rating.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/ProductScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/store.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/constants/productConstants.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/actions/productActions.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/Loader.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/Message.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/CartScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/reducers/productReducers.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/reducers/cartReducers.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/constants/cartConstants.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/actions/cartActions.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/reducers/userReducers.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/constants/userConstants.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/LoginScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/FormContainer.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/actions/userActions.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/RegisterScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/ProfileScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/ShippingScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/CheckoutSteps.js",[],["188","189"],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/PaymentScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/PlaceOrderScreen.js",["190"],"import React, { useEffect } from 'react';\nimport { Link } from 'react-router-dom';\nimport { Button, Row, Col, ListGroup, Image, Card } from 'react-bootstrap';\nimport { useDispatch, useSelector } from 'react-redux';\nimport Message from './../components/Message';\nimport CheckoutSteps from './../components/CheckoutSteps';\nimport { createOrder } from './../actions/orderActions'\n\nconst PlaceOrderScreen = ({ history }) => {\n  const dispatch = useDispatch()\n  const cart = useSelector(state => state.cart);\n  console.log(cart);\n\n  // calculate prices\n  const addDecimals = (num) => {\n    return (Math.round(num * 100)/100).toFixed(2);\n  }\n\n  cart.itemsPrice = addDecimals(cart.cartItems.reduce(\n    (acc, item) => acc + item.price * item.qty,\n    0\n  ))\n\n  cart.shippingPrice = addDecimals(cart.itemsPrice > 100 ? 0 : 100);\n  cart.taxPrice = addDecimals(Number((0.15 * cart.itemsPrice).toFixed(2)))\n  cart.totalPrice = (Number(cart.itemsPrice) + Number(cart.shippingPrice) + Number(cart.taxPrice)).toFixed(2)\n\n  const orderCreate = useSelector(state => state.orderCreate);\n  const { order, success, error } = orderCreate;\n\n  useEffect(() => {\n    if(success){\n      history.push(`/order/${order._id}`)\n    }\n  }, [history, success])\n\n  const placeOrderHandler = () => {\n    dispatch(createOrder({\n      orderItems: cart.cartItems,\n      shippingAddress: cart.shippingAddress,\n      paymentMethod: cart.paymentMethod,\n      itemsPrice: cart.itemsPrice,\n      shippingPrice: cart.shippingPrice,\n      taxPrice: cart.taxPrice,\n      totalPrice: cart.totalPrice\n    }))\n  }\n  return (\n    <>\n      <CheckoutSteps step1 step2 step3 step4 />\n      <Row>\n        <Col md={8}>\n          <ListGroup variant='flush'>\n            <ListGroup.Item>\n              <h2>Shipping</h2>\n              <p>\n                <strong>Address:</strong>\n                {cart.shippingAddress.address}, {cart.shippingAddress.city}{' '} {cart.shippingAddress.postalCode},{' '} {cart.shippingAddress.country}\n              </p>\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <h2>Payment Method</h2>\n              <strong>Method:</strong>\n              {cart.paymentMethod}\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Order Items</h2>\n              {cart.cartItems.length === 0 ? <Message>Your cart is empty</Message> : (\n                <ListGroup variant='flush'>\n                  {cart.cartItems.map((item, index) => (\n                    <ListGroup.Item key={index}>\n                      <Row>\n                        <Col md={1}>\n                          <Image src={item.image} alt={item.name} fluid rounded />\n                        </Col>\n                        <Col>\n                          <Link to={`/product/${item.product}`}>\n                            {item.name}\n                          </Link>\n                        </Col>\n                        <Col md={4}>\n                          {item.qty} x ${item.price} = ${item.qty * item.price}\n                        </Col>\n                      </Row>\n                    </ListGroup.Item>\n                  ))}\n                </ListGroup>\n              )}\n            </ListGroup.Item>\n          </ListGroup>\n        </Col>\n        <Col md={4}>\n          <Card>\n            <ListGroup variant='flush'>\n              <ListGroup.Item>\n                <h2>Order Summary</h2>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Items</Col>\n                  <Col>${cart.itemsPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Shipping</Col>\n                  <Col>${cart.shippingPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Tax</Col>\n                  <Col>${cart.taxPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Total</Col>\n                  <Col>${cart.totalPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                {error && <Message variant='danger'>{error}</Message>}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Button type='button' className='btn-block' disabled={cart.cartItems === 0} onClick={placeOrderHandler}>\n                  Place Order\n                </Button>\n              </ListGroup.Item>\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n    </>\n  )\n}\n\nexport default PlaceOrderScreen\n","/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/reducers/orderReducers.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/constants/orderConstants.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/actions/orderActions.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/OrderScreen.js",["191"],"import React, { useState, useEffect } from 'react';\nimport axios from 'axios';\nimport { PayPalButton } from 'react-paypal-button-v2';\nimport { Link } from 'react-router-dom';\nimport { Button, Row, Col, ListGroup, Image, Card } from 'react-bootstrap';\nimport { useDispatch, useSelector } from 'react-redux';\nimport Message from './../components/Message';\nimport Loader from './../components/Loader';\nimport { getOrderDetails, payOrder, deliverOrder } from './../actions/orderActions';\nimport { ORDER_PAY_RESET, ORDER_DELIVER_RESET } from './../constants/orderConstants';\n\nconst OrderScreen = ({ match, history }) => {\n  const [sdkReady, setSdkReady] = useState(false);\n  const orderId = match.params.id;\n  const dispatch = useDispatch()\n\n  const orderDetails = useSelector(state => state.orderDetails);\n  const { order, loading, error } = orderDetails;\n\n  const userLogin = useSelector(state => state.userLogin);\n  const { userInfo } = userLogin;\n\n  const orderPay = useSelector(state => state.orderPay);\n  // rename the properties like this\n  const { loading: loadingPay, success: successPay } = orderPay;\n\n  const orderDeliver = useSelector(state => state.orderDeliver);\n  const { loading: loadingDeliver, success: successDeliver } = orderDeliver;\n\n  if(!loading){\n      // calculate prices\n    const addDecimals = (num) => {\n      return (Math.round(num * 100)/100).toFixed(2);\n    }\n\n    order.itemsPrice = addDecimals(order.orderItems.reduce(\n      (acc, item) => acc + item.price * item.qty,\n      0\n    ))\n  }\n\n  useEffect(() => {\n    if(!userInfo){\n      history.push('/login');\n    }\n    const addPayPalScript = async() => {\n      const { data: clientId } = await axios.get('/api/config/paypal');\n      // console.log(clientId);\n      const script = document.createElement('script');\n      script.type = 'text/javascript';\n      script.src = `https://www.paypal.com/sdk/js?client-id=${clientId}`;\n      script.async = true;\n      script.onload = () => {\n        setSdkReady(true);\n      }\n      document.body.appendChild(script);\n    }\n    // addPayPalScript();\n\n  //  if(!order || order._id !== orderId || successPay){\n   if(!order || successPay || successDeliver){\n     dispatch({ type: ORDER_PAY_RESET })\n     dispatch({ type: ORDER_DELIVER_RESET })\n     dispatch(getOrderDetails(orderId))\n   } else if(!order.isPaid){\n     if(!window.paypal){\n       addPayPalScript()\n     } else {\n       setSdkReady(true)\n     }\n   }\n  }, [order, orderId, successPay, successDeliver])\n\n  const successPaymentHandler = (paymentResult) => {\n    console.log(paymentResult);\n    dispatch(payOrder(orderId, paymentResult));\n  }\n\n  const deliverHandler = () => {\n    dispatch(deliverOrder(order));\n  }\n\n  return loading ? \n  <Loader /> : error ? <Message variant='danger'>{error}</Message> :\n  <>\n    <h1>Order {order._id}</h1>\n    <Row>\n        <Col md={8}>\n          <ListGroup variant='flush'>\n            <ListGroup.Item>\n              <h2>Shipping</h2>\n              <p><strong>Name: </strong>{order.user.name}</p>\n              <p><strong>Email:</strong><a href={`mailto:${order.user.email}`}>{order.user.email}</a></p>\n              <p><strong>Address:</strong>\n                {order.shippingAddress.address}, {order.shippingAddress.city}{' '} {order.shippingAddress.postalCode},{' '} {order.shippingAddress.country}\n              </p>\n              {order.isDelivered ? <Message variant='success'>Delivered on {order.deliveredAt}</Message> : <Message variant='danger'>Not Delivered</Message>}\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <h2>Payment Method</h2>\n              <p>\n                <strong>Method:</strong>\n                {order.paymentMethod}\n              </p>\n              {order.isPaid ? <Message variant='success'>Paid on {order.paidAt}</Message> : <Message variant='danger'>Not paid</Message>}\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Order Items</h2>\n              {order.orderItems.length === 0 ? <Message>Order is empty</Message> : (\n                <ListGroup variant='flush'>\n                  {order.orderItems.map((item, index) => (\n                    <ListGroup.Item key={index}>\n                      <Row>\n                        <Col md={1}>\n                          <Image src={item.image} alt={item.name} fluid rounded />\n                        </Col>\n                        <Col>\n                          <Link to={`/product/${item.product}`}>\n                            {item.name}\n                          </Link>\n                        </Col>\n                        <Col md={4}>\n                          {item.qty} x ${item.price} = ${item.qty * item.price}\n                        </Col>\n                      </Row>\n                    </ListGroup.Item>\n                  ))}\n                </ListGroup>\n              )}\n            </ListGroup.Item>\n          </ListGroup>\n        </Col>\n        <Col md={4}>\n          <Card>\n            <ListGroup variant='flush'>\n              <ListGroup.Item>\n                <h2>Order Summary</h2>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Items</Col>\n                  <Col>${order.itemsPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Shipping</Col>\n                  <Col>${order.shippingPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Tax</Col>\n                  <Col>${order.taxPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Total</Col>\n                  <Col>${order.totalPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              {!order.isPaid && (\n                <ListGroup.Item>\n                  {loadingPay && <Loader />}\n                  {!sdkReady ? (\n                    <Loader />\n                  ) : (\n                    <PayPalButton amount={order.totalPrice} onSuccess={successPaymentHandler} />\n                  )}\n                </ListGroup.Item>\n              )}\n\n              {loadingDeliver && <Loader />}    \n              {userInfo && userInfo.isAdmin && order.isPaid && !order.isDelivered && (\n                <ListGroup.Item type='button' className='btn btn-block' onClick={deliverHandler}>\n                  <Button>Mark as Delivered</Button>\n                </ListGroup.Item>\n              )}\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n  </>\n}\n\nexport default OrderScreen\n","/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/UserListScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/UserEditScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/ProductListScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/ProductEditScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/screens/OrderListScreen.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/SearchBox.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/Paginate.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/ProductCarousel.js",[],"/Users/bj/dev/javascript/full-stack/shoppersChoice/frontend/src/components/Meta.js",[],{"ruleId":"192","replacedBy":"193"},{"ruleId":"194","replacedBy":"195"},{"ruleId":"192","replacedBy":"196"},{"ruleId":"194","replacedBy":"197"},{"ruleId":"192","replacedBy":"198"},{"ruleId":"194","replacedBy":"199"},{"ruleId":"200","severity":1,"message":"201","line":35,"column":6,"nodeType":"202","endLine":35,"endColumn":24,"suggestions":"203"},{"ruleId":"200","severity":1,"message":"204","line":72,"column":6,"nodeType":"202","endLine":72,"endColumn":50,"suggestions":"205"},"no-native-reassign",["206"],"no-negated-in-lhs",["207"],["206"],["207"],["206"],["207"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'order._id'. Either include it or remove the dependency